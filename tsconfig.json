{
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noUncheckedIndexedAccess": true,
    "typeRoots": ["./node_modules/@types"],
    "paths": {
      "@helpers/*": ["helpers/*"],
      "@fixtures/*": ["fixtures/*"],
      "@apiTypes/*": ["types/*"],
      "@pages/*": ["pages/*"]
    }
  }
}